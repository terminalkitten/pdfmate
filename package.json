{
  "name": "pdfmate",
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.py": [
      "isort -sp .isort",
      "git add",
      "black --skip-string-normalization",
      "git add",
      "flake8 --config=.flake8"
    ]
  },
  "dependencies": {
    "husky": "^4.3.0",
    "lint-staged": "^10.4.2"
  }
}
